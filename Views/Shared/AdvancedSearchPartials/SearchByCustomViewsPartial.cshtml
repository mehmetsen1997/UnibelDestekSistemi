


<div id="customSearchModal" class="modal fade bd-example-modal-lg" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">

        <div class="modal-content container-fluid">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel"><b>Choose Search Conditions</b></h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span style="font-size:26px" aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="row px-3 py-2">

                <div class="col-md-12 mt-4">
                    <h2>General Options</h2>
                </div>
                <div class="col-md-4 col-sm-6 col-xs-12">
                    <input id="customSearchCompany" type="checkbox" name="name" value="" onclick="checkBoxControl(this)" />
                    <label for="customSearchCompany" style="font-size:18px;margin-left:3px;">Company</label>
                </div>
                <div class="col-md-4 col-sm-6 col-xs-12">
                    <input id="customSearchStatus" type="checkbox" name="name" value="" onclick="checkBoxControl(this)" />
                    <label for="customSearchStatus" style="font-size:18px;margin-left:3px;">Status</label>
                </div>
                <div class="col-md-4 col-sm-6 col-xs-12">
                    <input id="customSearchType" type="checkbox" name="name" value="" onclick="checkBoxControl(this)" />
                    <label for="customSearchType" style="font-size:18px;margin-left:3px;">Type</label>
                </div>
                <div class="col-md-4 col-sm-6 col-xs-12">
                    <input id="customSearchPriority" type="checkbox" name="name" value="" onclick="checkBoxControl(this)" />
                    <label for="customSearchPriority" style="font-size:18px;margin-left:3px;">Priority</label>
                </div>
                <div class="col-md-4 col-sm-6 col-xs-12">
                    <input id="customSearchSubject" type="checkbox" name="name" value="" onclick="checkBoxControl(this)"/>
                    <label for="customSearchSubject" style="font-size:18px;margin-left:3px;">Subject</label>
                </div>
                <div class="col-md-4 col-sm-6 col-xs-12">
                    <input id="customSearchDepartment" type="checkbox" name="name" value="" onclick="checkBoxControl(this)" />
                    <label for="customSearchDepartment" style="font-size:18px;margin-left:3px;">Department</label>
                </div>

                <div class="col-md-4 col-sm-6 col-xs-12">
                    <input id="customSearchOwner" type="checkbox" name="name" value="" onclick="checkBoxControl(this)" />
                    <label for="customSearchOwner" style="font-size:18px;margin-left:3px;">Owner</label>
                </div>


                <div class="col-md-12 mt-4">
                    <h2>Other Options</h2>
                </div>
                <div class="col-md-4 col-sm-6 col-xs-12">
                    <input id="customSearchDemirbas" type="checkbox" name="name" value=""  onclick="checkBoxControl(this)"/>
                    <label for="customSearchDemirbas" style="font-size:18px;margin-left:3px;">Demirbaş</label>
                </div>
                <div class="col-md-4 col-sm-6 col-xs-12">
                    <input id="customSearchSeriNo" type="checkbox" name="name" value="" onclick="checkBoxControl(this)"/>
                    <label for="customSearchSeriNo" style="font-size:18px;margin-left:3px;">Seri No</label>
                </div>
                <div class="col-md-4 col-sm-6 col-xs-12">
                    <input id="customSearchUrun" type="checkbox" name="name" value="" onclick="checkBoxControl(this)"/>
                    <label for="customSearchUrun" style="font-size:18px;margin-left:3px;">Ürün</label>
                </div>
                <div class="col-md-4 col-sm-6 col-xs-12">
                    <input id="idCheckSearch" type="checkbox" name="name" value="" />
                    <label for="idCheckSearch" style="font-size:18px;margin-left:3px;">Due</label>
                </div>
                <div class="col-md-4 col-sm-6 col-xs-12">
                    <input id="idCheckSearch" type="checkbox" name="name" value="" />
                    <label for="idCheckSearch" style="font-size:18px;margin-left:3px;">Creator</label>
                </div>
                <div class="col-md-4 col-sm-6 col-xs-12">
                    <input id="idCheckSearch" type="checkbox" name="name" value="" />
                    <label for="idCheckSearch" style="font-size:18px;margin-left:3px;">Last Activity</label>
                </div>

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-success btn-lg" data-dismiss="modal">Kaydet</button>
            </div>
        </div>
    </div>
</div>



<div id="customSearchCompanyDiv" class="col-md-4 col-sm-6 col-xs-12" style="display:none;">
    
        <div class="col-md-12">

            <div class="row p-3">
                <div class="col-xl-12">
                    <label for="selectionPriority" style="font-size:14px;">Company list:</label>
                    <select class="form-control" id="customSelectionCompany" name="priority" style="font-size:16px;" onchange="customSelectionOfCompany()">
                        <option selected disabled>-- Select Company --</option>
                        @foreach (var company in ViewBag.company)
                        {
                            <option value="@company.SirketId">@company.SirketAdi</option>
                        }
                    </select>
                </div>
            </div>
            <div class="row p-3">
                <div class="col-xl-12">
                    <h4>Added Companies</h4>
                    <div class="card">
                        <div class="card-body">
                            <ul class="p-4" id="customSelectedCompanyList" style="list-style-type:disc;font-size:16px;"></ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    
</div>
