<div id="customSearchOwnerDiv" class="col-md-4 col-sm-6 col-xs-12" style="display:none;border">

    <div class="col-md-12">

        <div class="row p-3">
            <div class="col-xs-12">
                <label for="sel1" style="font-size:14px;">Department list:</label>
                <select class="form-control" id="customSelectionDepartmentOfOwner" name="status" style="font-size:16px;" onchange="customDepartmentSelectionOfOwner()">
                    <option value="all" selected>All</option>
                    @foreach (var department in ViewBag.department)
                    {
                        <option value="custom@@@department.DepartmanId">@department.DepartmanAdi</option>
                    }
                </select>
            </div>
        </div>

        <div class="row p-3">
            <div class="col-xs-12">
                <label for="sel1" style="font-size:14px;">Owner list:</label>
                <select class="form-control" id="customSelectionOwner" name="status" style="font-size:16px;" onchange="customSelectionOfOwner()">
                    <option style="display:none" selected disabled> -- Select Owner -- </option>
                    @foreach (var owner in ViewBag.owner)
                    {
                        <option class="custom@@@owner.FkDepartmanId" value="@owner.KullaniciId">@owner.KullaniciAdi</option>
                    }
                </select>
            </div>
        </div>

        <div class="row p-3">
            <div class="col-xs-12">
                <h4>Added Owners</h4>
                <div class="card">
                    <div class="card-body">
                        <ul class="p-4" id="customSelectedOwnerList" style="list-style-type:disc;font-size:16px;""></ul>
                    </div>
                </div>
            </div>
        </div>


        

    </div>

</div>
